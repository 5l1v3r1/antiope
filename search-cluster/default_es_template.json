{
  "index_patterns": ["resource*"],
  "mappings": {
    "_doc": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "account_id": {
          "type": "text"
        },
        "account_name": {
          "type": "text"
        },
        "resource_type": {
          "type": "keyword"
        },
        "last_seen": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss.SSSSSSZZ"

        }
      }
    }
  }
}